// VS Code의 자동완성 및 lint 설정을 위해서만 사용됩니다.
{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "dist/",
    "paths": {
      "@assets/*": ["./assets/*"],
      "@src/*": ["./src/*"]
    },
    "target": "esnext"
  },
  "exclude": ["node_modules"]
}
